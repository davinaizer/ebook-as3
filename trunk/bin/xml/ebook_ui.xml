				 hookSize="20"
				 autoSize="true"
				 followCursor="true"
				 align="center"
				 cornerRadius="10"
				 showDelay="100"
				 hideDelay="0"
				 bgAlpha="1"
				 width="350"
				 colors="0x353535,0x353535"
				 padding="10">

