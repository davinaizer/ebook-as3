			className="com.unboxds.ebook.view.components::InteractiveBarMeter"
			instanceName="progressMeter"

			x="50"
			y="596"
			width="900"
			height="4"

			bgColor="0xFFFFFF"
			bgColorAlpha=".75"

			barColor="0x008E00"
