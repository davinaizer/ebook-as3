		<content type="tooltip"
				 hook="true"
				 hookSize="20"
				 autoSize="true"
				 followCursor="true"
				 align="center"
				 cornerRadius="10"
				 showDelay="100"
				 hideDelay="0"
				 bgAlpha="1"
				 width="200"
				 colors="0x353535,0x353535"
				 padding="10">

			<content instanceName="progressCursor">
				<title><![CDATA[<p class="text-inverse">Você está na página <b>{$progress} de {$max}</b></p>]]></title>
				<body>
					<![CDATA[<p class="text-inverse">Você já percorreu <b>{$progressPercent}%</b> da sua jornada</p>]]>
				</body>
			</content>
		</content>

	</component>

