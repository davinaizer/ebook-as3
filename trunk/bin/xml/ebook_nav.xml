<?xml version="1.0" encoding="UTF-8"?>
<data title="UNBOX Learning Experience"
		history="false"
		indexFirst="true"
		flow="cross"
		menu="false"
		preloader="Preloader.swf">

	<!--
		 DATACONFIG OPTIONS
		 enableDebugPanel: true | false
		 enableAlerts: true | false
		 consultMode: true | false
		 dataServiceType: LocalStorage | ScormServer
		 outputXMLContent :true | false // Only use this option for outputting content. This should not be used in Final Version. It slows down the ebook loading.
		 scormReplaceDoubleQuotes: true | false // for compatibility among LMSs. Some accept only double quotes, other single.
	-->

	<config consultMode="false"
			enableAlerts="false"
			dataServiceType="ScormServer"
			scormReplaceDoubleQuotes="false"
			enableDebugPanel="true"
			outputXMLContent="false"
			version="eBookAS_V3">
		<activities>
			<maxScore>1,1</maxScore>
		</activities>
	</config>

